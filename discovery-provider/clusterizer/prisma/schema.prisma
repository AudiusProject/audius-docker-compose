generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("audius_db_url")
}

model RpcLog {
  cuid   String @id
  wallet String
  method String
  params Json

  jetstream_seq Int
  jetstream_ts  DateTime
  processed_at  DateTime

  @@map("rpc_log")
}
